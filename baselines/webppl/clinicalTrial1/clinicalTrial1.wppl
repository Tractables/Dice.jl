var model = function() {
    var isEffective = flip(0.5);
    var probControl = beta({a: 1, b:1});
    var probTreated = beta({a: 1, b:1});
    var probAll = beta({a: 1, b:1});

    map(
    function(c0, c1) {
        //var c0 = pair[0];
        //var c1 = pair[1];
        if (isEffective) {
            if (c0 == 0) {
                condition(flip(probControl) == false);
            }
            else {
                condition(flip(probControl) == true);
            }

            if (c1 == 0) {
                condition(flip(probTreated) == false);
            }
            else {
                condition(flip(probTreated) == true);
            }
        }
        else {
            if (c0 == 0) {
                condition(flip(probAll) == false);
            }
            else {
                condition(flip(probAll) == true);
            }

            if (c1 == 0) {
                condition(flip(probAll) == false);
            }
            else {
                condition(flip(probAll) == true);
            }
        }
    },
    pairs.slice(0, 100));

    return (isEffective);
}

